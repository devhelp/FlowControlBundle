{
    "name": "devhelp/flow-control-bundle",
    "type": "symfony-bundle",
    "description": "Provider integration with Devhelp/FlowControl library",
    "keywords": ["flow", "checkout"],
    "homepage": "http://devhelp.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Barański",
            "email": "pawel.baranski1@gmail.com"
        },
        {
            "name":     "devhelp.pl",
            "email": "devhelp@devhelp.pl"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2",
        "devhelp/flow-control": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~3",
        "squizlabs/php_codesniffer": "~1",
        "sebastian/phpcpd": "~2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Devhelp\\FlowControlBundle": "" }
    },
    "target-dir": "Devhelp/FlowControlBundle"
}
